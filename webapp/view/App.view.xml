<mvc:View
	controllerName="com.basf.techInterview.test1.CompanyEmployees.controller.App"
	displayBlock="true"
	height="100%"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:tnt="sap.tnt">
	<tnt:ToolHeader class="bgHeader">
		<Button icon="sap-icon://menu2" type="Transparent" />
		<Image src="./assets/logo.svg" width="76px" height="28px"/>
		<Text text="Test I by Srinivas Divakarla" wrapping="false" />
		<ToolbarSpacer />
	
		<Avatar src="./assets/me.png" displaySize="XS" />
		<Button icon="sap-icon://grid" type="Transparent" />
	</tnt:ToolHeader>
	<App
		id="app"
		busy="{appView>/busy}"
		busyIndicatorDelay="{appView>/delay}">
		<f:FlexibleColumnLayout
			id="layout"
			layout="{appView>/layout}"
			backgroundDesign="Translucent">
		</f:FlexibleColumnLayout>
	</App>
</mvc:View>